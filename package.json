{
  "name": "moysklad-desktop",
  "version": "0.6.5",
  "description": "moysklad desktop client",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder -p"
  },
  "author": "Maxim Filimonov",
  "license": "MIT",
  "homepage": "http://moysklad.ru",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maxim-Filimonov/moysklad-desktop.git"
  },
  "build": {
    "appId": "moysklad.ru",
    "mac": {
      "category": "business",
      "publish": [
        "github"
      ]
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "files": [
      "**/*",
      "build/icon.*"
    ]
  },
  "devDependencies": {
    "electron": "^5.0.8",
    "electron-builder": "^21.1.5"
  },
  "dependencies": {
    "electron-updater": "^4.1.2",
    "smalltalk": "^3.1.1"
  }
}
